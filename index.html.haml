!!! 5
%html(lang="en")
	%head
		%meta(charset="utf-8")
		%meta(http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1")
		%meta(name="viewport" content="width=device-width, initial-scale=1.0")
		%title= "Yang's Blog"
		/ Le HTML5 shim, for IE6-8 support of HTML elements
		/[if lt IE 9]
			%script(src="http://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js")
			%script(src="http://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js")
		%script(src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
		%script(src="http://cdn.knightlab.com/libs/timeline/latest/js/storyjs-embed.js")
		:javascript
			$(document).ready(function() {
				createStoryJS({
					type:       'timeline',
					width:      '800',
					height:     '600',
					source:     'data.js',
					embed_id:   'timeline-embed'
				});
			});
		%link{:type => "text/css", :rel => "stylesheet", :href => "http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css"}
	%body
		.container
			%h1 Welcome to Yang's Blog!
			#timeline-embed
